    <style name="tab_text" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/nunito_semibold</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="WrapContentDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="windowMinWidthMajor">0%</item>
        <item name="windowMinWidthMinor">0%</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
    </style>

    <style name="AppTheme.Button.Primary" parent="Widget.MaterialComponents.Button">
        <item name="android:backgroundTint">@color/kcb_blue</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="AppTheme.Button.Shimmer" parent="Widget.MaterialComponents.Button">
        <item name="android:backgroundTint">@color/shimmerColor</item>
        <item name="android:textColor">@color/shimmerColor</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="AppTheme.Button.Secondary" parent="Widget.MaterialComponents.Button">
        <item name="android:backgroundTint">@color/btn_gray</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="AppTheme.Button.Outlined" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="otpInputCustomWhite" parent="Widget.MaterialComponents.TextInputLayout.FilledBox.Dense">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textColorHint">@android:color/white</item>
        <item name="android:textSize">11sp</item>
        <item name="endIconTint">@android:color/white</item>
        <item name="boxBackgroundColor">#F7F7F7</item>
    </style>

    <style name="keypad_buttons_style" parent="Widget.MaterialComponents.TextView">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">32sp</item>
        <item name="android:textColor">#000</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:padding">10dp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:clickable">true</item>
        <item name="background">?attr/selectableItemBackgroundBorderless</item>
        <item name="android:foreground">?android:attr/selectableItemBackgroundBorderless</item>
        <item name="android:layout_width">0dp</item>
    </style>

    <style name="pin_views">
        <item name="android:layout_marginStart">12dp</item>
        <item name="android:background">@drawable/pin_inactive</item>
        <item name="android:layout_marginEnd">12dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:layout_marginTop">0dp</item>
        <item name="android:layout_width">20dp</item>
    </style>

    <style name="exposedDropdownMenuCustom" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="boxBackgroundColor">@android:color/transparent</item>
        <item name="android:radius">1.4dp</item>
        <item name="boxStrokeWidthFocused">1dp</item>
    </style>

    <style name="CustomAlertDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowBackground">@drawable/rounded_corners_btn</item>
    </style>

    <style name="AppBottomSheetDialogTheme" parent="Theme.Design.Light.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/AppModalStyle</item>
    </style>

    <style name="AppModalStyle" parent="Widget.Design.BottomSheet.Modal">
        <item name="android:background">@drawable/bsheetbg</item>
    </style>
    <style name="ShapeAppearanceOverlay.App.CornerSize50Percent" parent="">
        <item name="cornerSize">50%</item>
    </style>
